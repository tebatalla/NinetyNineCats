<h1><%= @cat.name %></h1>
<table>
  <thead>
    <tr>
      <% @cat_attributes.each do |k, v| %>
        <th><%= k %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% @cat_attributes.each do |k, v| %>
        <td><%= v %></td>
      <% end %>
    </tr>
  </tbody>
</table>
<br>
<%= link_to 'Edit ' + @cat.name, edit_cat_url(@cat) %>
<br>
<%= link_to 'Back to all cats', cats_url %>
